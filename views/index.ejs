<!DOCTYPE html>
<html>

<head>
  <title>
    <%= title %>
  </title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
  <div class="container">
    <nav>
      <ul>
        <li><a href="/">Home</a></li>
        <li><a href="/new">New Message</a></li>
      </ul>
    </nav>
    <hr>
    <h1>
      <%= title %>
    </h1>
    <% messages.forEach(function(message){ %>
      <div class="card">
        <p class="name">
          <%= message.user %>
        </p>
        <p>

          <%= message.text %>
        </p>
        <p>
          <%= message.added %>
        </p>
      </div>
      <% }); %>
  </div>
</body>

</html>